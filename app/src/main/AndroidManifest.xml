<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sandeep.womenincloudrvce">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.TripFactory_Concierge_Android"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".activity.DescriptionActivity"
            android:exported="false" />
        <activity
            android:name=".activity.EventActivity"
            android:exported="false" />
        <activity
            android:name=".activity.InternshipActivity"
            android:exported="false" />
        <activity
            android:name=".activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.PrivacyPolicyActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AccountActivity"
            android:exported="false" />
        <activity
            android:name=".activity.VoucherActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ProjectActivity"
            android:exported="false" />
        <activity
            android:name=".activity.RestRoomActivity"
            android:exported="false" />
        <activity
            android:name=".activity.PlaceActivity"
            android:exported="false" />
        <activity
            android:name=".activity.RestaurantActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ChatActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.CardDetailsActivity"
            android:screenOrientation="portrait"
        />

        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.MembersActivity"
            android:screenOrientation="portrait"
       />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.TaskListActivity"
            android:screenOrientation="portrait"
          />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.CreateBoardActivity"
            android:screenOrientation="portrait"
          />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.MyProfileActivity"
            android:screenOrientation="portrait"
          />
        <activity android:name="com.sandeep.womenincloudrvce.ui.activities.BaseActivity" />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.SigninActivity"
            android:screenOrientation="portrait"
          />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.SignupActivity"
            android:screenOrientation="portrait"
    />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.IntroActivity"
            android:screenOrientation="portrait"
           />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.SplashActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.sandeep.womenincloudrvce.ui.activities.MainActivity"
            android:screenOrientation="portrait"
/>

        <service
            android:name="com.sandeep.womenincloudrvce.fcm.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>